{
  "name": "starhackit-api",
  "description": "The starhackit backend",
  "version": "2.4.0",
  "private": false,
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:FredericHeem/starhackit.git"
  },
  "license": "NLPL",
  "scripts": {
    "devlabinstall": "lab install",
    "prestart": "npm run lint",
    "build": "gulp build:production",
    "start": "gulp",
    "pretest": "npm run build",
    "test": "npm run coverage",
    "mocha": "mocha ./src/**/test*.js ./test/**/test*.js",
    "coverage": "istanbul  cover -x 'src/**/test*.js' -x 'test/**/test*.js' _mocha --report text --report html --report lcov ./src/**/test*.js ./test/**/*.js",
    "coveralls": "coveralls < coverage/lcov.info",
    "codecov": "./node_modules/codecov.io/bin/codecov.io.js < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "lint": "eslint src",
    "migrate": "sequelize db:migrate",
    "start:prod": "pm2 start pm2.json --no-daemon",
    "clean": "gulp clean",
    "mock": "node scripts/mock-server.js",
    "api-check": "abao src/plugins/users/raml/users.raml --server http://localhost:9000/api/v1",
    "doc": "node scripts/apidoc.js",
    "opendoc": "open build/api.html"
  },
  "dependencies": {
    "axios": "0.13.1",
    "babel": "5.8.23",
    "babel-core": "5.8.25",
    "babel-eslint": "6.1.2",
    "babel-loader": "5.3.2",
    "babel-root-import": "1.0.1",
    "babel-runtime": "5.8.25",
    "bcryptjs": "2.3.0",
    "bignumber.js": "2.4.0",
    "bluebird": "3.4.1",
    "chance": "1.0.4",
    "config": "1.21.0",
    "ejs": "2.5.1",
    "eslint": "3.5.0",
    "eslint-plugin-mocha": "4.5.1",
    "gulp": "3.9.1",
    "gulp-babel": "5.2.1",
    "gulp-debug": "2.1.2",
    "gulp-mocha": "2.2.0",
    "gulp-rimraf": "0.2.0",
    "gulp-util": "3.0.7",
    "jsonschema": "1.1.0",
    "jsonwebtoken": "7.1.6",
    "kcors": "2.1.1",
    "koa": "2.0.0-alpha.3",
    "koa-66": "1.0.0",
    "koa-bodyparser": "3.1.0",
    "koa-convert": "1.2.0",
    "koa-generic-session": "1.11.3",
    "koa-passport": "2.2.0",
    "koa-redis": "2.1.2",
    "koa-static": "2.0.0",
    "lodash": "4.14.0",
    "logfilename": "0.1.3",
    "moment": "2.14.1",
    "nodemailer": "2.5.0",
    "passport-facebook": "2.1.1",
    "passport-jwt": "2.1.0",
    "passport-local": "1.0.0",
    "passport-oauth2": "1.3.0",
    "pg": "6.0.3",
    "pg-hstore": "2.3.2",
    "pm2": "1.1.3",
    "qs": "6.2.1",
    "rabbitmq-pubsub": "0.5.0",
    "redis": "2.6.2",
    "run-sequence": "1.2.2",
    "sequelize": "3.24.3",
    "socket.io": "1.4.8",
    "sqlite3": "3.1.4",
    "urijs": "1.18.1",
    "validator": "5.5.0",
    "webpack": "1.13.1",
    "winston": "2.2.0"
  },
  "devDependencies": {
    "abao": "0.4.0-beta.8",
    "async": "2.0.1",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "codeclimate-test-reporter": "0.3.3",
    "codecov.io": "0.1.6",
    "coveralls": "2.11.11",
    "devlab": "2.0.2",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "mocha": "2.5.3",
    "raml-mocker-server": "0.1.6",
    "raml2html": "3.0.0",
    "restauth": "1.1.1",
    "sequelize-cli": "2.4.0",
    "shelljs": "0.7.3",
    "should": "10.0.0",
    "sinon": "1.17.4",
    "sinon-as-promised": "4.0.2"
  },
  "engines": {
    "node": ">=4"
  }
}
